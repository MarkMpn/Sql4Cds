<UserControl x:Class="MarkMpn.Sql4Cds.ScriptDom.DebugVisualizer.DebuggerSide.ScriptDomUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf">
    <DockPanel>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="5" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <wv2:WebView2 Name="webView" NavigationCompleted="WebViewNavigationCompleted">
            </wv2:WebView2>
            <GridSplitter Grid.Row="1" Height="5" HorizontalAlignment="Stretch" />
            <TreeView Name="treeView" Grid.Row="2" />
        </Grid>
    </DockPanel>
</UserControl>