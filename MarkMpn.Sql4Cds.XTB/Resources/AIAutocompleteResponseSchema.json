{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Contains a list of possible autocomplete suggestions",
  "type": "object",
  "properties": {
    "completions": {
      "type": "array",
      "uniqueItems": true,
      "minItems": 1,
      "items": {
        "required": [
          "sql"
        ],
        "properties": {
          "sql": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    }
  },
  "required": [
    "completions"
  ]
}